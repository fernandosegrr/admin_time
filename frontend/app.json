{
    "expo": {
        "name": "Gesti√≥n de Tiempo",
        "slug": "gestion-tiempo",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1a1a2e"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.gestiontiempo.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#1a1a2e"
            },
            "package": "com.gestiontiempo.app"
        },
        "plugins": [
            "expo-router",
            [
                "expo-notifications",
                {
                    "icon": "./assets/notification-icon.png",
                    "color": "#4285F4"
                }
            ]
        ],
        "scheme": "gestiontiempo",
        "extra": {
            "eas": {
                "projectId": "your-project-id"
            }
        }
    }
}